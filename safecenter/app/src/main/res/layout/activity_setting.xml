<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/text_title"
        android:text="设置" />

    <com.tangqi.safecenter.view.SettingItemCb
        android:id="@+id/si_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.tangqi.safecenter.view.SettingItemCb

        android:id="@+id/number_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.tangqi.safecenter.view.SettingItemArrow
        xmlns:safecenter="http://schemas.android.com/apk/res/com.tangqi.safecenter"
        android:id="@+id/location_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.tangqi.safecenter.view.SettingItemArrow
        android:id="@+id/location_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <com.tangqi.safecenter.view.SettingItemCb
        android:id="@+id/black_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.tangqi.safecenter.view.SettingItemCb
        android:id="@+id/app_lock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>
